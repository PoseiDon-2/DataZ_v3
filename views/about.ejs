<style>
  body {
    background-color: #f3f4f6;
    font-family: 'Arial', sans-serif;
    font-family: 'fc_font', sans-serif;
  }
  @font-face {
    font-family: 'fc_font';
    src: url('/fontsty/FC Quantum/FC Quantum [Non-commercial].ttf') format('truetype');
}

  .team-section {
    padding: 60px 0;
    background-color: #ffffff;
  }

  .section-title {
    font-family: 'fc_font', sans-serif;
    text-align: center;
    font-size: 2.5rem;
    font-weight: bold;
    color: #ffffff;
    margin-bottom: 50px;
  }

  .team-member {
    text-align: center;
    margin-bottom: 40px;
    margin-left: 40px;
    margin-right: 40px;
    position: relative;
    overflow: hidden;
    border-radius: 15px;
    background-color: #ffffff;
    box-shadow: 0px 4px 15px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease-in-out;
  }

  .team-member:hover {
    transform: scale(1.05);
  }

  .member-photo {
    width: 100%;
    height: 250px;
    object-fit: cover;
    border-top-left-radius: 15px;
    border-top-right-radius: 15px;
    margin-top: 10px;
  }

  .member-info {
    padding: 20px;
  }

  .member-name {
    font-size: 1.5rem;
    font-weight: bold;
    color: #ff0000;
    margin-bottom: 10px;
  }

  .member-role {
    font-size: 1.2rem;
    color: #6a0202;
    margin-bottom: 10px;
  }

  .member-description {
    font-size: 1rem;
    color: #424242;
    margin-bottom: 20px;
  }

  .social-icons {
    display: flex;
    justify-content: center;
    gap: 15px;
  }

  .social-icons a {
    font-size: 1.5rem;
    color: #ff0000;
    transition: color 0.3s ease;
  }

  .social-icons a:hover {
    color: #570101;
  }
</style>
</head>

